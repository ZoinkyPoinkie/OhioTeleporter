local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

OrionLib:MakeNotification({
	Name = "Ohio.",
	Content = "Teleporter Script By xtc4lyfe",
	Image = "rbxassetid://4483345998",
	Time = 5
})


local Window = OrionLib:MakeWindow({Name = "Ohio. Teleporter", HidePremium = false, SaveConfig = true, ConfigFolder = "Orion"})

--Player Tab--

local PlayerTab = Window:MakeTab({
	Name = "Teleports",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})



local PlayerSection = PlayerTab:AddSection({
	Name = "Most Important"
})

PlayerSection:AddButton({
Name = "open bank Vault",
	Callback = function()
      		fireproximityprompt(workspace.BankRobbery.VaultDoor.Door.Attachment.ProximityPrompt)
  	end    
})

PlayerSection:AddButton({
Name = "AUG",
	Callback = function()
      		local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local goal = {CFrame = CFrame.new(Vector3.new(1185.526001, 44.285511, -764.075256))}

local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local tween = TweenService:Create(humanoidRootPart, tweenInfo, goal)
tween:Play()

  	end    
})

